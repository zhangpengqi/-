@import "../../../common.wxss";
.searchPage {
  display: flex;
  flex-direction: row;
  justify-content: space-between; }

.searchPage .input {
  display: flex;
  flex-direction: row;
  width: 85%;
  background-color: #F2F2F2;
  align-items: center;
  padding: 10rpx 3%;
  height: 50rpx;
  border-radius: 50rpx;
  margin-left: 2%; }

.searchPage .iconfont.icon-search {
  font-size: 40rpx;
  margin-right: 1%; }

.searchPage .return {
  align-items: center;
  justify-content: center;
  text-align: center;
  width: 15%;
  height: 50rpx;
  line-height: 50rpx;
  padding: 10rpx 0;
  background-color: #0077aa; }

